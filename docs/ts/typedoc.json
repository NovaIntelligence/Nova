{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Nova Bot TypeScript Documentation",
  "entryPoints": [
    "../../**/*.ts",
    "../../**/*.js"
  ],
  "exclude": [
    "../../node_modules/**/*",
    "../../dist/**/*",
    "../../build/**/*",
    "../../coverage/**/*",
    "../../artifacts/**/*",
    "../../.git/**/*",
    "../../tests/**/*.spec.ts",
    "../../tests/**/*.test.ts"
  ],
  "out": "../site/ts",
  "theme": "default",
  "readme": "../../README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeExternals": false,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Modules", 
    "Dashboard",
    "Metrics",
    "Skills",
    "Security",
    "Utilities",
    "Types",
    "Interfaces",
    "Other"
  ],
  "sort": [
    "source-order",
    "required-first",
    "alphabetical"
  ],
  "kindSortOrder": [
    "Document",
    "Project", 
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Method",
    "Parameter",
    "TypeParameter",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "ObjectLiteral",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "ConditionalType",
    "Reference"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "titleLink": "https://NovaIntelligence.github.io/Nova",
  "navigationLinks": {
    "GitHub": "https://github.com/NovaIntelligence/Nova",
    "Documentation": "https://NovaIntelligence.github.io/Nova",
    "PowerShell Docs": "../ps/README.html",
    "Python Docs": "../py/index.html"
  },
  "sidebarLinks": {
    "Nova Bot Home": "https://NovaIntelligence.github.io/Nova",
    "GitHub Repository": "https://github.com/NovaIntelligence/Nova",
    "Report Issues": "https://github.com/NovaIntelligence/Nova/issues"
  },
  "hostedBaseUrl": "https://NovaIntelligence.github.io/Nova/ts/",
  "hideGenerator": false,
  "customCss": "./typedoc-custom.css",
  "plugin": [
    "typedoc-plugin-mermaid",
    "typedoc-plugin-missing-exports"
  ],
  "lightHighlightTheme": "github-light",
  "darkHighlightTheme": "github-dark",
  "cname": "NovaIntelligence.github.io",
  "githubPages": true,
  "gitRevision": "master",
  "gitRemote": "origin",
  "sourceLinkTemplate": "https://github.com/NovaIntelligence/Nova/blob/{gitRevision}/{path}#L{line}",
  "disableSources": false,
  "basePath": "../../",
  "excludeTags": [
    "@internal",
    "@alpha"
  ],
  "blockTags": [
    "@author",
    "@since",
    "@version",
    "@see",
    "@example",
    "@deprecated",
    "@experimental",
    "@beta",
    "@alpha",
    "@internal",
    "@override",
    "@virtual",
    "@sealed",
    "@remarks",
    "@public",
    "@protected",
    "@private",
    "@readonly",
    "@eventProperty",
    "@defaultValue",
    "@typeParam",
    "@template"
  ],
  "inlineTags": [
    "@link",
    "@linkcode",
    "@linkplain",
    "@inheritDoc"
  ],
  "modifierTags": [
    "@override",
    "@virtual",
    "@sealed",
    "@deprecated",
    "@experimental",
    "@beta",
    "@alpha",
    "@internal",
    "@readonly",
    "@eventProperty"
  ],
  "categorizeByGroup": true,
  "groupOrder": [
    "Classes",
    "Interfaces", 
    "Type Aliases",
    "Variables",
    "Functions",
    "Enums"
  ],
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "requiredToBeDocumented": [
    "Enum",
    "EnumMember", 
    "Variable",
    "Function",
    "Class",
    "Interface",
    "Property",
    "Method",
    "Accessor",
    "TypeAlias"
  ],
  "commentStyle": "jsdoc",
  "useTsLinkResolution": true,
  "preserveWatchOutput": false,
  "skipErrorChecking": false,
  "emit": "docs",
  "logLevel": "Info",
  "listInvalidSymbolLinks": false,
  "markedOptions": {
    "mangle": false,
    "breaks": false,
    "gfm": true,
    "tables": true,
    "smartLists": true,
    "smartypants": false
  },
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "../../",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}